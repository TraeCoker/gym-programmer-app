<div class="wrapper">
<div style="text-align: center">
    <h1>Programs</h1>
    <br>
    <ul>
        <%i = 0%>
        <%@programs.each do |program|%>
            <li><b><a href="/programs/<%=program.id%>"><%=program.name%></a> a <%=program.days_per_week%> days a week program by <a href="/user/<%=@users[i]%>"><%=@users[i]%></a></b></li>
            <br>
            <%i += 1%>
        <%end%>
    </ul><br><br>
    </div>
</div>