<div class="wrapper">
    <h1>Programs</h1>
    <ul>
        <%i = 0%>
        <%@programs.each do |program|%>
            <li><a href="/programs/<%=program.id%>"><%=program.name%></a> a <%=program.days_per_week%> days a week program by <a href="/user/<%=@users[i]%>"><%=@users[i]%></a></li>
            <%i += 1%>
        <%end%>
    </ul><br><br>
</div>